{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "semicolon": [true, "never"],
        "variable-name": [true, "allow-snake-case"],
        "no-console": [false],
        "max-classes-per-file": [false, 1],
        "no-empty": [true, "allow-empty-catch"]
    },
    "rulesDirectory": []
}